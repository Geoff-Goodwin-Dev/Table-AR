{
  "name": "Table-AR",
  "version": "1.0.0",
  "description": "Table AR will utilize a combination of Augmented Reality and video processing to enable effective task management",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": "git@github.com:Geoff-Goodwin-Dev/Table-AR.git",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Geoff-Goodwin-Dev/Table-AR/issues"
  },
  "homepage": "https://github.com/Geoff-Goodwin-Dev/Table-AR#readme"
}
